#exportFrame{
	background: #888888;
	top:0px;
	left:60px;
	bottom:60px;
	right: 60px;
	position:absolute;
	display:none;
	z-index:20;	
	box-shadow: 0px -5px 15px rgba(0,0,0,0.5); 
}
#exportHeader{
	width:100%;
	height:60px;
	background: none;
	z-index: 20;
	box-shadow: 0px -5px 15px rgba(0,0,0,0.5);
}

#exportCenterImageColumn{
	width:300px;
	bottom: 0px;
	top: 60px;
	position: absolute;
	background: none;
	z-index: 20;
}

.imageThumbnailExport{
	height: 18px;
	border: 1px solid black;	
}

#exportCenterImageColumnLeftCheckboxes{
	width: 35px;
	top:0px;
	bottom: 0px;
	left: 0px;
	position: absolute;
	background: none;
	z-index: 21;
}

#exportCenterImageColumnThumbnailColumn{
	position: absolute;
	left: 35px;
	right: 35px;
	top: 0px;
	bottom: 0px;
	background: none;
	z-index: 20;
}

.exportCenterImageColumnCheckbox{
	width: 33px !important;
	height: 33px !important;
	border: 1px !important;
	position: absolute !important;
	top: 0px;
	left: 0px;
}

#exportCenterImageColumnRightCheckboxes{
	width: 35px;
	top:0px;
	bottom: 0px;
	right: 0px;
	position: absolute;
	background: none;
	z-index: 21;
}

#exportCenterTopButtons{
	height:60px;
	width: 320px;
	top: 60px;
	left: 600px;
	position: absolute;
	background: none;
	z-index: 20;
}

#exportCenterBodyMap{
	top: 120px;
	position: absolute;
	bottom: 60px;
	right: 0px;
	left: 300px;
	background: none;
	z-index: 20;	
}
#exportCenterBottomButtons{
	height:60px;
	width:480px;
	background: none;
	bottom:0px;
	left: 600px;
	position: absolute;
	z-index: 20;
}

#exportBottomButton{
	width: 160px;
	height: 60px;
	bottom: 0px;
	background: none;
	left: 600px;
	position: absolute;
	display: none;
	z-index: 19;
}

.exportCenterTopButtonsStyle{
	width: 160px;
	height: 60px;
	background: url('../../resources/images/btnSelectallNormal.png') no-repeat 100% 100% ;
	color: rgb(0, 0, 0);
	font-family: "Helvetica Neue 75Bold", Arial, sans-serif;
	font-size: 10pt;
	margin-right: -1px;
	background-size: cover;
}

.exportCenterBottomButtonsStyle{
	width: 160px;
	height: 60px;
	background: url('../../resources/images/btnImagesNormal.png') no-repeat 100% 100% ;
	color: rgb(0, 0, 0);
	font-family: "Helvetica Neue 75Bold", Arial, sans-serif;
	font-size: 14pt;
	margin-right: -1px;
	background-size: cover;
}

.exportBodymapImage{
	position: absolute !important;	
}

.exportBottomButton {
	width: 160px;
	height: 61px;
	background: url('../../resources/images/btnInfoActive.png') no-repeat 100% 100% ;
	color: rgb(0, 0, 0);
	font-family: "Helvetica Neue 75Bold", Arial, sans-serif;
	font-size: 14pt;
	margin-right: -1px;
	background-size: cover;
}

.exportCloseButton{
	width: 60px;
	height: 60px;
	position: absolute;
	right: 0px;
	background: url('../../resources/images/btnCloseNormal.png') no-repeat 100% 100% ;
