<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
<h:head />

<body>
<ui:composition>
	<div id="exportFrame">
		<div id="exportHeader">
		<h:outputText value="EXPORT" />
		<p:commandButton value="" onclick="hideExportLayer()" styleClass="exportCloseButton"/>
		</div>		
		
		<div id="exportCenterImageColumn">
			<div id="exportCenterImageColumnLeftCheckboxes">
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxOne" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftOne"/>
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxTwo" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftTwo"/>
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxThree" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftThree"/>
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxFour" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftFour"/>
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxFive" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftFive"/>
				<p:selectBooleanCheckbox value="false" id="leftCheckBoxSix" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarLeftSix"/>
			</div>
			
			<div id="exportCenterImageColumnThumbnailColumn">
				<p:graphicImage id="exportThumbnailOne" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />
				<p:graphicImage id="exportThumbnailTwo" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />
				<p:graphicImage id="exportThumbnailThree" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailFour" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />
				<p:graphicImage id="exportThumbnailFive" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailSix" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailSeven" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailEight" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailNine" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailTen" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailEleven" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
				<p:graphicImage id="exportThumbnailTwelve" styleClass="imageThumbnailExport" 
						value="resources/images/imageThumbnailNormal.png" />	
			</div>			
			<div id="exportCenterImageColumnRightCheckboxes">
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxOne" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightOne"/>
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxTwo" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightTwo"/>
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxThree" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightThree"/>
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxFour" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightFour"/>
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxFive" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightFive"/>
				<p:selectBooleanCheckbox value="false" id="rightCheckBoxSix" styleClass="exportCenterImageColumnCheckbox" widgetVar="exportCenterImageColumnCheckboxWidgetVarRightSix"/>
			</div>
		</div>
		
		<div id="exportCenterTopButtons">
			<p:commandButton value="SELECT ALL" styleClass="exportCenterTopButtonsStyle" onclick="selectAll()"/>
			<p:commandButton value="DESELECT ALL" styleClass="exportCenterTopButtonsStyle" onclick="deselectAll()"/>
		</div>
		
		<div id="exportCenterBodyMap">
			<p:graphicImage value="resources/images/bodymapFemaleLeft.png" id="exportBodymapLeft" styleClass="exportBodymapImage"/>
			<p:graphicImage value="resources/images/bodymapFemaleFront.png" id="exportBodymapFront" styleClass="exportBodymapImage"/>
			<p:graphicImage value="resources/images/bodymapFemaleRight.png" id="exportBodymapRight" styleClass="exportBodymapImage"/>
			<p:graphicImage value="resources/images/bodymapFemaleBack.png" id="exportBodymapBack" styleClass="exportBodymapImage"/>
		</div>
		
		<div id="exportCenterBottomButtons">
			<p:commandButton value="DB" styleClass="exportCenterBottomButtonsStyle" />
			<p:commandButton value="RECORD" styleClass="exportCenterBottomButtonsStyle" />
			<p:commandButton value="IMAGES" styleClass="exportCenterBottomButtonsStyle" />
		</div>		
	</div>
	
	<div id="exportBottomButton">
		<p:commandButton value="EXPORT" onclick="hideExportLayer()" styleClass="exportBottomButton" />
	</div>
</ui:composition>
</body>
</html>